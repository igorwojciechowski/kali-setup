{
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "'Hack Nerd Font',Consolas, 'Courier New', monospace",
    "editor.fontWeight": "bold",
    "vim.useCtrlKeys": false,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Pylance",
    "workbench.iconTheme": "vscode-icons",
    "editor.minimap.enabled": false,
    "workbench.colorCustomizations": {

        "foreground": "#f7f7f7",
        "menu.background": "#191919",

        // Editor
        "editor.foreground": "#f7f7f7",
        "editor.background": "#191919",
        "editorGroupHeader.tabsBackground": "#191919",
        "editorLineNumber.foreground": "#4d4d4d",
        "editorLineNumber.activeForeground": "#8d8d8d",

        // Panel
        "panel.background": "#191919",

        // Buttons
        "button.background": "#191919",

        // Status bar
        "statusBar.background": "#191919",

        // Side bar
        "sideBar.background": "#191919",
        "sideBarSectionHeader.background": "#191919",

        // Banner
        "banner.background": "#191919",

        // Title bar
        "titleBar.activeBackground": "#191919",
        "titleBar.inactiveBackground": "#191919",
        "tab.activeBackground": "#2a2a2a",
        "tab.inactiveBackground": "#191919",

        // Activity bar
        "activityBar.background": "#191919",
        "activityBar.activeBackground": "#191919",

        // Git
        "gitDecoration.modifiedResourceForeground": "#de5959",
        "gitDecoration.untrackedResourceForeground": "#de5959",

    },
